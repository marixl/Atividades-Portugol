programa {


  funcao inicio() {
    para (inteiro i = 1; i <= 10;i++) {
      escreva(i, "\n")
    }

    escreva ("contagem regressiva de 10 a 1. /n")

    para (inteiro i = 10; i>=1; i--){
      escreva (i, "\n")
    }
    escreva ("contando de 1 a 10. \n incrementando em 2\n")
    para( inteiro i = 1; i<10; i+=2){
      escreva(i, "\n")
    }
    
  }
}

